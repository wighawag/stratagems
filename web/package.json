{
  "name": "stratagems-web",
  "version": "0.0.1",
  "description": "Frontend for stratagems.",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.5.0",
