    "viem": "^2.7.12",
    "web3-connection": "^0.1.39",
    "web3-connection-viem": "^0.2.0"
  },
  "scripts": {
    "prepare": "pwag static/icon.png src/web-config.json",
    "dev": "echo 'waiting for src/data/contracts.ts...'; wait-on src/data/contracts.ts && ldenv vite dev",
    "build": "pnpm run prepare && ldenv vite build",
    "preview": "ldenv vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "svelte-check": "svelte-check",
    "serve": "ipfs-emulator --only -d build -p 8080"
  }
