{
    "name": "fuzd-service",
    "devDependencies": {
        "fuzd-cf-worker": "^0.0.30",
        "ldenv": "^0.3.9",
        "node-cron": "^3.0.3",
        "set-defaults": "^0.0.2",
        "toml": "^3.0.0",
        "wait-on": "^7.2.0",
        "wrangler": "^3.28.4"
    },
    "scripts": {
        "prepare": "set-defaults .",
        "dev:cron": "node cron.js",
        "dev:worker": "echo 'waiting for contracts.json...'; wait-on contracts.json && node generate-env.js && wrangler dev -c ./wrangler.toml --port 34002",
        "dev": "pnpm run /^dev:.*/",
        "wrangler": "wrangler"
    }
}